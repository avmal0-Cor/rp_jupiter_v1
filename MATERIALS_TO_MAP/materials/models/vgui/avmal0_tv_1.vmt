VertexLitGeneric
{
    $basetexture    models/vgui/avmal0_tv_1
    $selfillum      1
    $halflambert    1
    $selfillumtint  "[.5 .5.5]"
    
    Proxies
    {
        AnimatedTexture
        {
            animatedtexturevar          $basetexture
            animatedtextureframenumvar  $frame
            animatedtextureframerate    5
        }
    }
}